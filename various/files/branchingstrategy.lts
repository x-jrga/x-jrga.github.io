MASTER=(add_branch_DEVELOP->DEVELOP|add_branch_HOTFIX->HOTFIX),
HOTFIX=(commit_HOTFIX_fix->HOTFIX|merge_into_MASTER->MASTER|merge_into_DEVELOP->DEVELOP),
DEVELOP=(add_branch_FEATURE->FEATURE|commit_DEVELOP_fix->DEVELOP|add_branch_RELEASE->RELEASE),
FEATURE=(commit_FEATURE_fix->FEATURE|merge_into_DEVELOP->DEVELOP),
RELEASE=(commit_RELEASE_fix->RELEASE|merge_into_MASTER->MASTER|merge_into_DEVELOP->DEVELOP).

menu RUN = {branch_to_develop}
